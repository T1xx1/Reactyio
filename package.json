      "husky": "^8.0.1",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "size-limit": "^8.1.0",
      "tsdx": "^0.14.1",
      "tslib": "^2.4.0",
      "typescript": "^3.9.10"
   },
   "size-limit": [
